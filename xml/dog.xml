<?xml version="1.0" encoding="UTF-8"?>
<mujoco model="snake">
    <compiler angle="degree" />
    <option timestep="0.001"
            gravity="0 0 -9.81" />
    <default>
        <equality solref="0.005 1" />
        <geom type="box"
              size="0.05 0.02 0.02"
              density="500"
              rgba="0.2 0.7 0.2 1" />
        <joint limited="true"
               range="-300 300"
               damping="0.2"
               stiffness="5" />
        <motor ctrllimited="true"
               ctrlrange="-3 3" />
    </default>
    <worldbody>
        <!-- Segment 0 (root body) -->
        <body name="body"
              pos="0 0 0.3">
            <freejoint name="root_free" />
            <geom name="body_geom"
                  type="box"
                  size="0.2 0.03 0.03"
                  pos="0 0 0"
                  rgba="1 0 0 1" />
            <!-- Left Front Shoulder -->
            <body name="shoulder_lf"
                  pos="-0.2 -0.05 0">
                <joint name="lf_upper_shoulder"
                       type="hinge"
                       axis="1 0 0"
                       pos="0 0.02 -0.02" />
                  <site name="u_shoulder_joint_lf"
                          pos="0 0.02 -0.02"
                          size="0.01"
                          rgba="0 0.25 0 1" />
                <geom name="shoulder_lf_geom"
                      type="box"
                      size="0.05 0.02 0.02"
                      pos="0 0 0"
                      rgba="1 1 0 1" />
                <!-- Left Front Upper Leg -->
                <body name="upper_leg_lf"
                      pos="0 -0.03 -0.05">
                    <joint name="lf_lower_shoulder"
                           type="hinge"
                           axis="0 1 0"
                           pos="0 0 0.05" />
                    <site name="shoulder_joint_lf"
                          pos="0 0 0.05"
                          size="0.01"
                          rgba="0 0.5 0 1" />
                    <geom name="upper_leg_lf_geom"
                          type="box"
                          size="0.01 0.01 0.05"
                          pos="0 0 0"
                          rgba="0 1 0 1" />
                    <!-- Left Front Lower Leg -->
                    <body name="lower_leg_lf"
                          pos="0 0.02 -0.10">
                        <joint name="lf_knee"
                               type="hinge"
                               axis="0 1 0"
                               pos="0 -0.01 0.05" />
                        <site name="knee_joint_lf"
                              pos="0 -0.01 0.05"
                              size="0.01"
                              rgba="0 0.5 0.5 1" />
                        <geom name="lower_leg_lf_geom"
                              type="box"
                              size="0.01 0.01 0.05"
                              pos="0 0 0"
                              rgba="0 1 1 1" />
                        <!-- Left front Foot -->
                        <body name="foot_lf"
                              pos="0 0 -0.05">
                            <geom name="foot_lf_geom"
                                  type="sphere"
                                  size="0.02"
                                  pos="0 0 0" />
                        </body>
                    </body>
                </body>
            </body>

            <!-- Right Front Shoulder -->
            <body name="shoulder_rf"
                  pos="-0.2 0.05 0">
                <joint name="rf_upper_shoulder"
                       type="hinge"
                       axis="1 0 0"
                       pos="0 -0.02 -0.02" />
                  <site name="u_shoulder_joint_rf"
                          pos="0 -0.02 -0.02"
                          size="0.01"
                          rgba="0 0.25 0 1" />
                <geom name="shoulder_rf_geom"
                      type="box"
                      size="0.05 0.02 0.02"
                      pos="0 0 0"
                      rgba="1 1 0 1" />
                <!-- Right Front Upper Leg -->
                <body name="upper_leg_rf"
                      pos="0 0.03 -0.05">
                    <joint name="rf_lower_shoulder"
                           type="hinge"
                           axis="0 1 0"
                           pos="0 0 0.05" />
                    <site name="shoulder_joint_rf"
                          pos="0 0 0.05"
                          size="0.01"
                          rgba="0 0.5 0 1" />
                    <geom name="upper_leg_rf_geom"
                          type="box"
                          size="0.01 0.01 0.05"
                          pos="0 0 0"
                          rgba="0 1 0 1" />
                    <!-- Right Front Lower Leg -->
                    <body name="lower_leg_rf"
                          pos="0 -0.02 -0.10">
                        <joint name="rf_knee"
                               type="hinge"
                               axis="0 1 0"
                               pos="0 0.01 0.05" />
                        <site name="knee_joint_rf"
                              pos="0 0.01 0.05"
                              size="0.01"
                              rgba="0 0.5 0.5 1" />
                        <geom name="lower_leg_rf_geom"
                              type="box"
                              size="0.01 0.01 0.05"
                              pos="0 0 0"
                              rgba="0 1 1 1" />
                        <!-- Right Front Foot -->
                        <body name="foot_rf"
                              pos="0 0 -0.05">
                            <geom name="foot_rf_geom"
                                  type="sphere"
                                  size="0.02"
                                  pos="0 0 0" />
                        </body>
                    </body>
                </body>
            </body>

            <!-- Left Back Shoulder -->
            <body name="shoulder_lb"
                  pos="0.2 -0.05 0">
                <joint name="lb_upper_shoulder"
                       type="hinge"
                       axis="1 0 0"
                       pos="0 0.02 -0.02" />
                  <site name="u_shoulder_joint_lb"
                          pos="0 0.02 -0.02"
                          size="0.01"
                          rgba="0 0.25 0 1" />
                <geom name="shoulder_lb_geom"
                      type="box"
                      size="0.05 0.02 0.02"
                      pos="0 0 0"
                      rgba="1 1 0 1" />
                <!-- Left Back Upper Leg -->
                <body name="upper_leg_lb"
                      pos="0 -0.03 -0.05">
                    <joint name="lb_lower_shoulder"
                           type="hinge"
                           axis="0 1 0"
                           pos="0 0 0.05" />
                    <site name="shoulder_joint_lb"
                          pos="0 0 0.05"
                          size="0.01"
                          rgba="0 0.5 0 1" />
                    <geom name="upper_leg_lb_geom"
                          type="box"
                          size="0.01 0.01 0.05"
                          pos="0 0 0"
                          rgba="0 1 0 1" />
                    <!-- Left Back Lower Leg -->
                    <body name="lower_leg_lb"
                          pos="0 0.02 -0.10">
                        <joint name="lb_knee"
                               type="hinge"
                               axis="0 1 0"
                               pos="0 -0.01 0.05" />
                        <site name="knee_joint_lb"
                              pos="0 -0.01 0.05"
                              size="0.01"
                              rgba="0 0.5 0.5 1" />
                        <geom name="lower_leg_lb_geom"
                              type="box"
                              size="0.01 0.01 0.05"
                              pos="0 0 0"
                              rgba="0 1 1 1" />
                        <!-- Left Back Foot -->
                        <body name="foot_lb"
                              pos="0 0 -0.05">
                            <geom name="foot_lb_geom"
                                  type="sphere"
                                  size="0.02"
                                  pos="0 0 0" />
                        </body>
                    </body>
                </body>
            </body>

            <!-- Right Back Shoulder -->
            <body name="shoulder_rb"
                  pos="0.2 0.05 0">
                  <joint name="rb_upper_shoulder"
                       type="hinge"
                       axis="1 0 0"
                       pos="0 -0.02 -0.02" />
                  <site name="u_shoulder_joint_rb"
                          pos="0 -0.02 -0.02"
                          size="0.01"
                          rgba="0 0.25 0 1" />
                <geom name="shoulder_rb_geom"
                      type="box"
                      size="0.05 0.02 0.02"
                      pos="0 0 0"
                      rgba="1 1 0 1" />
                <!-- Right Back Upper Leg -->
                <body name="upper_leg_rb"
                      pos="0 0.03 -0.05">
                    <joint name="rb_lower_shoulder"
                           type="hinge"
                           axis="0 1 0"
                           pos="0 0 0.05" />
                    <site name="shoulder_joint_rb"
                          pos="0 0 0.05"
                          size="0.01"
                          rgba="0 0.5 0 1" />
                    <geom name="upper_leg_rb_geom"
                          type="box"
                          size="0.01 0.01 0.05"
                          pos="0 0 0"
                          rgba="0 1 0 1" />
                    <!-- Right Back Lower Leg -->
                    <body name="lower_leg_rb"
                          pos="0 -0.02 -0.10">
                        <joint name="rb_knee"
                               type="hinge"
                               axis="0 1 0"
                               pos="0 0.01 0.05" />
                        <site name="knee_joint_rb"
                              pos="0 0.01 0.05"
                              size="0.01"
                              rgba="0 0.5 0.5 1" />
                        <geom name="lower_leg_rb_geom"
                              type="box"
                              size="0.01 0.01 0.05"
                              pos="0 0 0"
                              rgba="0 1 1 1" />
                        <!-- Right Back Foot -->
                        <body name="foot_rb"
                              pos="0 0 -0.05">
                            <geom name="foot_rb_geom"
                                  type="sphere"
                                  size="0.02"
                                  pos="0 0 0" />
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <actuator>
        <!-- Left Front -->
      <motor name="lf_u_shoulder_motor"
               joint="lf_upper_shoulder"
               forcerange="-20 20" />
        <motor name="lf_l_shoulder_motor"
               joint="lf_lower_shoulder"
               forcerange="-20 20" />
        <motor name="lf_knee_motor"
               joint="lf_knee"
               forcerange="-20 20" />
        <!-- Right Front -->
      <motor name="rf_u_shoulder_motor"
               joint="rf_upper_shoulder"
               forcerange="-20 20" />
        <motor name="rf_l_shoulder_motor"
               joint="rf_lower_shoulder"
               forcerange="-20 20" />
        <motor name="rf_knee_motor"
               joint="rf_knee"
               forcerange="-20 20" />
        <!-- Left Back -->
              <motor name="lb_u_shoulder_motor"
               joint="lb_upper_shoulder"
               forcerange="-20 20" />
        <motor name="lb_l_shoulder_motor"
               joint="lb_lower_shoulder"
               forcerange="-20 20" />
        <motor name="lb_knee_motor"
               joint="lb_knee"
               forcerange="-20 20" />
        <!-- Right Back -->
              <motor name="rb_u_shoulder_motor"
               joint="rb_upper_shoulder"
               forcerange="-20 20" />
        <motor name="rb_l_shoulder_motor"
               joint="rb_lower_shoulder"
               forcerange="-20 20" />
        <motor name="rb_knee_motor"
               joint="rb_knee"
               forcerange="-20 20" />
    </actuator>

    <sensor>
        <jointpos joint="lf_lower_shoulder" />
        <jointpos joint="lf_knee" />
    </sensor>
</mujoco>